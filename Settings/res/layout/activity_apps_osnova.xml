<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/background_fon"
    android:orientation="vertical" >
    
    <ua.mkh.settings.full.EdgeEffectScrollView
        android:id="@+id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:fadingEdge="none"
        android:overScrollMode="always"
        android:scrollbars="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/LinearLayoutMem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/ImageView01"
                android:layout_width="fill_parent"
                android:layout_height="1px"
                android:ems="10"
                android:scaleType="fitXY"
                android:src="@drawable/line_cray" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="@color/white" >

                <ImageView
                    android:id="@+id/ImageView11"
                    android:layout_width="fill_parent"
                    android:layout_height="1px"
                    android:layout_alignLeft="@+id/Button08"
                    android:layout_alignParentBottom="true"
                    android:ems="10"
                    android:paddingLeft="15dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/line_cray" />

                <TextView
                    android:id="@+id/TextView07"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:hint="TextView"
                    android:text="Unknow"
                    android:textColor="@color/hint_text"
                    android:textSize="15sp" />

                <Button
                    android:id="@+id/Button08"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:background="@color/transparent"
                    android:clickable="false"
                    android:ems="10"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="15dp"
                    android:text="@string/storage_zanyato"
                    android:textColor="@color/black"
                    android:textSize="15sp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="@color/white" >

                <ImageView
                    android:id="@+id/ImageView12"
                    android:layout_width="fill_parent"
                    android:layout_height="1px"
                    android:layout_alignParentBottom="true"
                    android:ems="10"
                    android:paddingLeft="15dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/line_cray" />

                <TextView
                    android:id="@+id/TextView08"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:gravity="right"
                    android:hint="TextView"
                    android:text="Unknow"
                    android:textColor="@color/hint_text"
                    android:textSize="15sp" />

                <Button
                    android:id="@+id/Button09"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:background="@color/transparent"
                    android:clickable="false"
                    android:ems="10"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="15dp"
                    android:text="@string/storage_dostupno"
                    android:textColor="@color/black"
                    android:textSize="15sp" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/ImageView02"
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:scaleType="fitXY"
        android:src="@drawable/line_cray" />
 
    <ListView
        android:id="@android:id/list"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:divider="@color/hint_text"
        android:drawSelectorOnTop="true" />

    <ImageView
        android:id="@+id/ImageView03"
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:scaleType="fitXY"
        android:src="@drawable/line_cray" />
    
     </LinearLayout>

    </ua.mkh.settings.full.EdgeEffectScrollView>
 
<LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/background_title"
        android:orientation="vertical" >

        <include
            android:id="@+id/include1"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            layout="@layout/title_status" />
    </LinearLayout>

</RelativeLayout>
